set number
" set relativenumber
set ai
" set cursorline
set bg=dark
set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab
set hlsearch

filetype indent on

" inoremap ( ()<Esc>i
" inoremap " ""<Esc>i
" inoremap ' ''<Esc>i
" inoremap [ []<Esc>i

" inoremap {<CR> {<CR>}<Esc>ko
" inoremap {{ {}<Esc>i
